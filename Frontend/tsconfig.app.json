{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "types": ["vite/client"],
    "noEmit": false,  // ✅ 显式设置为 false，覆盖继承的配置

    /* Linting */
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true,
    "noUncheckedSideEffectImports": true
  },
  "include": ["src/**/*.ts", "src/**/*.tsx", "src/**/*.vue", "src/packages/data-panel/managers/SchemaManager.ts", "src/packages/data-panel/managers/DataManager.ts", "src/packages/data-panel/components/TopicDetailPanel.vue", "src/packages/data-panel/components/TopicList.vue", "packages/data-panel/utils/protoParser.ts"]
}